generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Post {
  id        String   @id @default(cuid()) @map("_id")
  thumbnail String?
  postId    String   @default(cuid())
  title     String
  contents  String?
  createdAt DateTime @default(now())
}
